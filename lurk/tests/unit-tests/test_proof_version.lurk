!(load "../../src/lib.lurk")

!(def gamma_ok '(31 0 1))
!(def gamma_fail '(31 3 10))

!(def check_version_in_gamma_phase (lambda (input)
    (let ((stack Stack)
          (memory Memory)
          (claims Claims)
          (result (switch_case Gamma_phase stack memory claims input))
         )
         (if (eq result nil)
            nil
            t
         )
    )
))

(check_version_in_gamma_phase gamma_ok) ;; should return t
(check_version_in_gamma_phase gamma_fail) ;; should return nil
